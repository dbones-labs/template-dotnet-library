{
    "$schema": "http://json.schemastore.org/template",
    "author": "dbones labs",
    "classifications": [
        "dotnet",
        "library",
        "lab"
    ],
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "identity": "DBones.Template.DotNet.Library",
    "name": "single library solution setup",
    "shortName": "dotnet-lib",
    "sourceName": "ProjectName",

    "symbols": {
