            "type": "parameter",
            "datatype": "text",
            "replaces": "organisation-name",
            "defaultValue": "dbones-labs"
        },
        "project-name": {
            "type": "parameter",
            "datatype": "text",
            "replaces": "project-name",
            "defaultValue": ""
        }
    }
